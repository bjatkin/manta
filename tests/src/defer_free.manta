// examples/defer_free.manta

type ErrWrite enum {
    Ok
    IOError
}

fn write_and_cleanup(path string) ErrWrite {
    .Ok(f) := open(path) catch { return .IOError }
    defer { close(f) }

    buf := new([]u8, 256)
    defer { free(buf) }

    .Ok(n) := write(f, buf) catch { return .IOError }
    return .Ok
}
