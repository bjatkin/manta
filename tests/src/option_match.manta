// examples/option_match.manta
mod main

use (
    "fmt"
)

// Div is the return type for the div function
type Div enum {
    Ok(i32)
    DivByZero
}

// div tries to divide the given integers. If b is 0 the function returns .DivByZero
fn div(a, b i32) Div {
    if b == 0 {
        return .DivByZero
    }
    return .Ok(a / b)
}

fn main() {
    let r = div(10, 2)
    match r {
        .Ok(v) { fmt::println("result: ", v) }
        .DivByZero { fmt::println("can not divide by zero") }
    }
}
