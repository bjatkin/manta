// examples/if_else.manta
mod main

use (
    "fmt"
)

// TODO: replace with fmt::println once modules are supported
fn fmt_println(s str) {}

// test if our if statments work
fn main() {
    let check_this = true

    if check_this {
        fmt_println("yes")
    }

    let nine = 9

    if nine > 10 {
        fmt_println("no")
    } else {
        fmt_println("yes")
    }
}

